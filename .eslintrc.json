{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "no-console": "warn",
    "react/prop-types": "warn",
    "object-curly-newline": "off",
    "react/forbid-prop-types": "off"
  },
  "eslintIgnore": ["*.test.js"]
}
